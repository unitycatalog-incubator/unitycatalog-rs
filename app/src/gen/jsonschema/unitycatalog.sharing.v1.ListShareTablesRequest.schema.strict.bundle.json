{
  "$defs": {
    "unitycatalog.sharing.v1.ListShareTablesRequest.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "List tables in a share.",
      "properties": {
        "max_results": {
          "description": "The maximum number of results per page that should be returned.",
          "exclusiveMaximum": 1000,
          "exclusiveMinimum": 0,
          "type": "integer"
        },
        "name": {
          "description": "The share name to query. It's case-insensitive.",
          "minLength": 1,
          "pattern": "^[a-z][0-9a-z._]*[0-9a-z]$",
          "type": "string"
        },
        "page_token": {
          "description": "Specifies a page token to use. Set pageToken to the nextPageToken returned\n by a previous list request to get the next page of results.",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "List Share Tables Request",
      "type": "object"
    }
  },
  "$id": "unitycatalog.sharing.v1.ListShareTablesRequest.schema.strict.bundle.json",
  "$ref": "#/$defs/unitycatalog.sharing.v1.ListShareTablesRequest.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
