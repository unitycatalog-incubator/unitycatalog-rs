{
  "$defs": {
    "unitycatalog.credentials.v1.ListCredentialsRequest.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(maxResults)$": {
          "anyOf": [
            {
              "exclusiveMaximum": 1000,
              "exclusiveMinimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "description": "The maximum number of results per page that should be returned."
        },
        "^(pageToken)$": {
          "description": "Opaque pagination token to go to next page based on previous query.",
          "type": "string"
        }
      },
      "properties": {
        "max_results": {
          "anyOf": [
            {
              "exclusiveMaximum": 1000,
              "exclusiveMinimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "description": "The maximum number of results per page that should be returned."
        },
        "page_token": {
          "description": "Opaque pagination token to go to next page based on previous query.",
          "type": "string"
        },
        "purpose": {
          "anyOf": [
            {
              "enum": [
                "PURPOSE_UNSPECIFIED",
                "STORAGE",
                "SERVICE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "description": "Return only credentials for the specified purpose.",
          "title": "Purpose"
        }
      },
      "title": "List Credentials Request",
      "type": "object"
    }
  },
  "$id": "unitycatalog.credentials.v1.ListCredentialsRequest.schema.bundle.json",
  "$ref": "#/$defs/unitycatalog.credentials.v1.ListCredentialsRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
