{
  "$defs": {
    "unitycatalog.recipients.v1.ListRecipientsRequest.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Request to list recipients.",
      "patternProperties": {
        "^(maxResults)$": {
          "anyOf": [
            {
              "exclusiveMaximum": 1000,
              "exclusiveMinimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "description": "The maximum number of results per page that should be returned."
        },
        "^(pageToken)$": {
          "description": "Opaque pagination token to go to next page based on previous query.",
          "type": "string"
        }
      },
      "properties": {
        "max_results": {
          "anyOf": [
            {
              "exclusiveMaximum": 1000,
              "exclusiveMinimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "description": "The maximum number of results per page that should be returned."
        },
        "page_token": {
          "description": "Opaque pagination token to go to next page based on previous query.",
          "type": "string"
        }
      },
      "title": "List Recipients Request",
      "type": "object"
    }
  },
  "$id": "unitycatalog.recipients.v1.ListRecipientsRequest.schema.bundle.json",
  "$ref": "#/$defs/unitycatalog.recipients.v1.ListRecipientsRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
