{
  "$defs": {
    "unitycatalog.recipients.v1.RecipientToken.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(activationUrl)$": {
          "default": "",
          "description": "Full activation URL to retrieve the access token. It will be empty if the token is already retrieved.",
          "type": "string"
        },
        "^(createdAt)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Time at which this recipient token was created, in epoch milliseconds."
        },
        "^(createdBy)$": {
          "default": "",
          "description": "Username of recipient token creator.",
          "type": "string"
        },
        "^(expirationTime)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Expiration timestamp of the token in epoch milliseconds."
        },
        "^(updatedAt)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Time at which this recipient token was updated, in epoch milliseconds."
        },
        "^(updatedBy)$": {
          "default": "",
          "description": "Username of recipient token updater.",
          "type": "string"
        }
      },
      "properties": {
        "activation_url": {
          "default": "",
          "description": "Full activation URL to retrieve the access token. It will be empty if the token is already retrieved.",
          "type": "string"
        },
        "created_at": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Time at which this recipient token was created, in epoch milliseconds."
        },
        "created_by": {
          "default": "",
          "description": "Username of recipient token creator.",
          "type": "string"
        },
        "expiration_time": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Expiration timestamp of the token in epoch milliseconds."
        },
        "id": {
          "default": "",
          "description": "Unique ID of the recipient token.",
          "type": "string"
        },
        "updated_at": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Time at which this recipient token was updated, in epoch milliseconds."
        },
        "updated_by": {
          "default": "",
          "description": "Username of recipient token updater.",
          "type": "string"
        }
      },
      "title": "Recipient Token",
      "type": "object"
    }
  },
  "$id": "unitycatalog.recipients.v1.RecipientToken.schema.bundle.json",
  "$ref": "#/$defs/unitycatalog.recipients.v1.RecipientToken.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
