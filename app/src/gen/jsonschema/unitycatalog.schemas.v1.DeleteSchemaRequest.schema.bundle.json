{
  "$defs": {
    "unitycatalog.schemas.v1.DeleteSchemaRequest.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Delete a Schema",
      "patternProperties": {
        "^(fullName)$": {
          "default": "",
          "description": "Format: catalog_name.schema_name",
          "pattern": "^[a-z][0-9a-z._]*[0-9a-z]$",
          "title": "Full name of schema to delete.",
          "type": "string"
        }
      },
      "properties": {
        "force": {
          "description": "Force deletion even if the schema is not empty.",
          "type": "boolean"
        },
        "full_name": {
          "default": "",
          "description": "Format: catalog_name.schema_name",
          "pattern": "^[a-z][0-9a-z._]*[0-9a-z]$",
          "title": "Full name of schema to delete.",
          "type": "string"
        }
      },
      "title": "Delete Schema Request",
      "type": "object"
    }
  },
  "$id": "unitycatalog.schemas.v1.DeleteSchemaRequest.schema.bundle.json",
  "$ref": "#/$defs/unitycatalog.schemas.v1.DeleteSchemaRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
