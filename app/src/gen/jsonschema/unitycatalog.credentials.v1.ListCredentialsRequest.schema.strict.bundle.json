{
  "$defs": {
    "unitycatalog.credentials.v1.ListCredentialsRequest.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "max_results": {
          "description": "The maximum number of results per page that should be returned.",
          "exclusiveMaximum": 1000,
          "exclusiveMinimum": 0,
          "type": "integer"
        },
        "page_token": {
          "description": "Opaque pagination token to go to next page based on previous query.",
          "type": "string"
        },
        "purpose": {
          "description": "Return only credentials for the specified purpose.",
          "enum": [
            "PURPOSE_UNSPECIFIED",
            "STORAGE",
            "SERVICE"
          ],
          "title": "Purpose",
          "type": "string"
        }
      },
      "title": "List Credentials Request",
      "type": "object"
    }
  },
  "$id": "unitycatalog.credentials.v1.ListCredentialsRequest.schema.strict.bundle.json",
  "$ref": "#/$defs/unitycatalog.credentials.v1.ListCredentialsRequest.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
