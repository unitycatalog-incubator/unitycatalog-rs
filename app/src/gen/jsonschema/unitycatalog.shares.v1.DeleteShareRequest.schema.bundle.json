    "unitycatalog.shares.v1.DeleteShareRequest.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "The caller must be a metastore admin or have the DELETE_SHARE privilege on the metastore.",
      "properties": {
        "name": {
          "default": "",
          "description": "Name of the share.",
          "minLength": 3,
          "pattern": "^[a-z][0-9a-z._]*[0-9a-z]$",
          "type": "string"
        }
      },
      "title": "Delete a share.",
      "type": "object"
    }
  },
  "$id": "unitycatalog.shares.v1.DeleteShareRequest.schema.bundle.json",
  "$ref": "#/$defs/unitycatalog.shares.v1.DeleteShareRequest.schema.json",
