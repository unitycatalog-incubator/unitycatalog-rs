{
  "$defs": {
    "unitycatalog.catalogs.v1.DeleteCatalogRequest.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Delete a catalog",
      "properties": {
        "force": {
          "description": "Force deletion even if the catalog is not empty.",
          "type": "boolean"
        },
        "name": {
          "description": "Name of catalog.",
          "minLength": 3,
          "pattern": "^[a-z][0-9a-z_]*[0-9a-z]$",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "Delete Catalog Request",
      "type": "object"
    }
  },
  "$id": "unitycatalog.catalogs.v1.DeleteCatalogRequest.schema.strict.bundle.json",
  "$ref": "#/$defs/unitycatalog.catalogs.v1.DeleteCatalogRequest.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
